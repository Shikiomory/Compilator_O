MODULE IsSquare;
(* Является ли число квадратом *)
IMPORT In, Out;
VAR
  k, n, s: INTEGER;
BEGIN
  In.Open;
  In.Int(n);
  k := 1;
  s := 0;
  WHILE s < n DO
    s := s + k;
    k := k + 2;
  END;
  IF s = n THEN
    Out.Int(1, 0)
  ELSE
    Out.Int(0, 0)
  END;
  Out.Ln;
END IsSquare.
(* 
1 + 3 + 5 + 7 + ... (2*k - 1) = 2k*k/2 = k*k = k^2
*)
